{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "governance-mode.vocab.core-1.0.0",
  "title": "frontmatter/overlays/governance-mode.vocab.core",
  "$defs": {
    "coreToken": {
      "type": "string",
      "enum": ["strict", "advisory"]
    },
    "token": {
      "oneOf": [
        { "$ref": "#/$defs/coreToken" },
        {
          "type": "string",
          "pattern": "^x-[A-Za-z][A-Za-z0-9_-]*$",
          "maxLength": 64
        }
      ]
    }
  }
}
