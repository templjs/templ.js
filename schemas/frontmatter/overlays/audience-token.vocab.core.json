{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "audience-token.vocab.core-1.0.0",
  "title": "frontmatter/overlays/audience-token.vocab.core",
  "$defs": {
    "coreToken": {
      "type": "string",
      "enum": [
        "integrators",
        "contributors",
        "operators",
        "endUsers",
        "administrators",
        "executives"
      ]
    },
    "token": {
      "oneOf": [
        { "$ref": "#/$defs/coreToken" },
        {
          "type": "string",
          "pattern": "^x-[a-z0-9]+(?:-[a-z0-9]+)*$",
          "maxLength": 64
        }
      ]
    }
  }
}
