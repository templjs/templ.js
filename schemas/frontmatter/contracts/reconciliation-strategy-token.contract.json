{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "reconciliation-strategy-token.contract-1.0.0",
  "title": "frontmatter/contracts/reconciliation-strategy-token",
  "description": "Extension contract for reconciliation strategy tokens. Supports canonical tokens and x-* extension tokens.",
  "oneOf": [
    {
      "type": "string",
      "pattern": "^x-[A-Za-z][A-Za-z0-9_-]*$",
      "maxLength": 64
    },
    {
      "type": "string",
      "pattern": "^[a-z][a-z0-9-]*$",
      "not": {
        "pattern": "^x-[A-Za-z][A-Za-z0-9_-]*$"
      },
      "maxLength": 64
    }
  ]
}
